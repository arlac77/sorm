#!/usr/bin/env node

"use strict";

var commander= require('commander')
   ,fs       = require('fs')
   ,os       = require("os")
   ,util     = require('util')
   ,path     = require('path')
   ,async    = require('async')
   ,pkginfo  = require('pkginfo')(module, 'name', 'version')
   ,sqlite3  = require('sqlite3')
   ,schema   = require('../lib/schema.js');


commander
  	.version(module.exports.version)
  	.parse(process.argv);


var s = schema.Schema("tests/test2re.schema");

var db = new sqlite3.Database("tests/reverse.db");

s.load_ddl_from_db(db,function(error) {
	
	s.save(function(error) {
		if(error)
			console.log("save error: " + error);
		else
			console.log("saved: " + s.basename);
	});
});